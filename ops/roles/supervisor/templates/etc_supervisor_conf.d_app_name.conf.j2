[program:{{app_name}}]
command=/usr/bin/dotnet {{deploy_directory}}/{{entry_dll_file_name}}
directory={{deploy_directory}}
autostart=true
autorestart=true
stderr_logfile=/var/log/{{app_name}}.err.log
stdout_logfile=/var/log/{{app_name}}.out.log
environment=HOME=/var/www/,ASPNETCORE_ENVIRONMENT=Production
user=www-data
stopsignal=INT
stopasgroup=true
killasgroup=true